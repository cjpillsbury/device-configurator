{
  "name": "foldable-device-configurator",
  "version": "1.1.5",
  "description": "A simple web component which you can add to emulate various type of foldables devices. It will set the appropriate media queries according to the device and the setup.",
  "main": "build/foldable-device-configurator.js",
  "module": "build/foldable-device-configurator.js",
  "snowpack": {
    "webDependencies": [
      "lit-element",
      "lit-html",
      "lit-html/directives/class-map.js",
      "@material/mwc-slider",
      "@material/mwc-checkbox",
      "@material/mwc-drawer",
      "@material/mwc-icon-button",
      "spanning-css-polyfill"
    ],
    "dedupe": [
      "lit-element",
      "lit-html",
      "spanning-css-polyfill"
    ]
  },
  "scripts": {
    "start": "es-dev-server --node-resolve --watch --babel --open demo/index.html --port 8081",
    "build": "rimraf build && rollup -c rollup.config.js && rimraf web_modules && snowpack --optimize",
    "regen-package-lock": "rimraf node_modules && rimraf package-lock.json; npm install"
  },
  "files": [
    "!/demo/",
    "!/src/"
  ],
  "dependencies": {
    "@material/mwc-slider": "^0.17.2",
    "@material/mwc-drawer": "^0.17.2",
    "@material/mwc-icon-button": "^0.17.2",
    "@material/mwc-checkbox": "^0.17.2",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "spanning-css-polyfill": "^1.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-babel": "^5.1.0",
    "es-dev-server": "^1.56.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.21.0",
    "snowpack": "^2.6.4"
  },
  "keywords": [],
  "type": "module",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Alexis Menard",
    "email": "alexis.menard@intel.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foldable-devices/device-configurator.git"
  }
}
